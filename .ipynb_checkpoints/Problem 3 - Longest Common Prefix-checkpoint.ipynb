   "source": [
    "strs=[\"flower\",\"flow\",\"flight\"]\n",
    "# strs=[\"reflower\",\"flow\",\"flight\"]\n",
    "lens=[len(s) for s in strs]\n",
    "small_ind=lens.index(min(lens))\n",
    "prefix=strs.pop(small_ind)\n",
    "\n",
    "while len(prefix)>0:\n",
    "    if all(prefix in s[:len(prefix)] for s in strs):\n",
    "        break\n",
    "    prefix=prefix[:-1]\n",
    "\n",
    "print(prefix)"
   ]
