   "source": [
    "def isPalindrome(x: int) -> bool:\n",
    "    if x<0:\n",
    "        return False\n",
    "    elif x==0:\n",
    "        return True\n",
    "    face_values=[int(i) for i in str(x)]\n",
    "    return face_values==face_values[::-1]"
   ]
