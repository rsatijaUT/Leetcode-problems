   "source": [
    "def maxSubArray(nums):\n",
    "    global_max = float('-inf')\n",
    "    local_max = 0\n",
    "    for num in nums:\n",
    "        local_max = max(num, num+local_max)\n",
    "        global_max = max(global_max, local_max)\n",
    "    return global_max\n",
    "\n",
    "nums = [-2,1,-3,4,-1,2,1,-5,4]\n",
    "maxSubArray(nums)"
   ]
