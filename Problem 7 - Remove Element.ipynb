   "source": [
    "def removeElement(nums,val):\n",
    "    i=0\n",
    "    for i<len(nums):\n",
    "        if nums[i]==val:\n",
    "            nums.pop(i)\n",
    "            i=i-1\n",
    "        i=i+1\n",
    "    return len(nums)\n",
    "\n",
    "nums=[3,2,2,3]\n",
    "val=3\n",
    "k=removeElement(nums,val)\n",
    "print(k,nums)"
   ]
  }
