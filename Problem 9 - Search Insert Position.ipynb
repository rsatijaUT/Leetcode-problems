   "source": [
    "def searchInsert(nums,target):\n",
    "    l,r = 0, len(nums)\n",
    "    while l < r:\n",
    "        m = (l+r) // 2\n",
    "        if nums[m] == target:\n",
    "            return m\n",
    "        if nums[m] < target:\n",
    "            l = m + 1\n",
    "        else:\n",
    "            r = m\n",
    "    return l\n",
    "\n"
